<Window x:Class="SampleDataGenerator.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SampleDataGenerator"
        xmlns:localUI="clr-namespace:SampleDataGenerator.UI"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800"
        d:DataContext="{d:DesignInstance Type= {x:Type localUI:MainViewModel}}">
    <Grid>
        <Button Content="Generate" Command="{ Binding GenerateSampleData}"/>
    </Grid>
</Window>
